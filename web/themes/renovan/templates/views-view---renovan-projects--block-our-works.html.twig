{% if rows %}
    {% if footer %}
        {{ footer }}
    {% endif%}
    {% if header %}
        <div class="container">
            <div class="row">
                <div class="col-xs-12">
                    {{ header }}
                </div>
            </div>
        </div>
    {% endif %}            
    <div class="grid">
        {% for row in rows %}
            {{ row }}
        {% endfor %}
    </div>

{% endif %}

